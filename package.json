  "dependencies": {
    "cors": "^2.8.6",
    "express": "^4.22.1",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill-new": "^3.8.3",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "react-scripts": "^5.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
